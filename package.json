    "jest": "29.0.3",
    "ts-jest": "29.0.2",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "zod": "^3.19.1"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --build --watch --incremental",
